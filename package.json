{
  "name": "preact-starter",
  "type": "module",
  "version": "1.0.0",
  "description": "Barebones starter for a preact project",
  "main": "dist/index.js",
  "scripts": {
    "build": "vite build",
    "serve": "vite preview",
    "start": "vite",
    "lint": "pnpm run tsc && pnpm run check-format",
    "tsc": "tsc -b tsconfig.json",
    "check-format": "prettier --ignore-unknown --check .",
    "format": "prettier --ignore-unknown --write .",
    "prepare": "node scripts/prepare.ts"
  },
  "author": "Andre Wiggins",
  "license": "MIT",
  "dependencies": {
    "preact": "^10.22.1"
  },
  "devDependencies": {
    "@preact/preset-vite": "^2.10.2",
    "@types/node": "^24.10.1",
    "prettier": "^3.6.2",
    "typescript": "^5.9.3",
    "vite": "^7.2.2"
  },
  "volta": {
    "node": "24.11.1"
  }
}
